                  <Font name="Comic Sans MS" size="20.0" />
               </font>
            </Label>
            <TableView fx:id="tblTipoAluguel" layoutX="7.0" layoutY="77.0" onMouseClicked="#selecionaTipoAluguel" prefHeight="316.0" prefWidth="587.0">
              <columns>
                <TableColumn fx:id="tbcDescricao" prefWidth="253.0" text="Descrição" />
                <TableColumn fx:id="tbcValor" prefWidth="166.0" text="Valor" />
                  <TableColumn fx:id="tbcTaxa" prefWidth="167.0" text="Taxa" />
              </columns>
            </TableView>
         </children>
      </Pane>
